<?xml version="1.0"?>
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"	
	xml:base = "http://www.sport.com">	
	
	<!-- Deficion de clases -->
		<rdfs:Class rdf:ID="person"/>	
		<rdfs:Class rdf:ID="sportleague"/>
		<rdfs:Class rdf:ID="sportteam"/>
		<rdfs:Class rdf:ID="sportclub"/>
		<rdfs:Class rdf:ID="teammember">
			<rdfs:subClassOf rdf:resource="#person"/>
		</rdfs:Class>
		<rdfs:Class rdf:ID="sportmanager">
			<rdfs:subClassOf rdf:resource="#person"/>
		</rdfs:Class>	
	
	<!-- definicion de propiedades -->
		<rdf:Property rdf:ID="nit">			
			<rdfs:domain rdf:resource="#sportleague"/>
			<rdfs:domain rdf:resource="#sportclub"/>
			<rdfs:domain rdf:resource="#sportteam"/>
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
		<!-- definicion de propiedades  sportleague,sportclub,sportteam-->
		<rdf:Property rdf:ID="name">
			<rdfs:domain rdf:resource="#person"/>
			<rdfs:domain rdf:resource="#sportleague"/>
			<rdfs:domain rdf:resource="#sportclub"/>
			<rdfs:domain rdf:resource="#sportteam"/>
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="numberfteams">
			<rdfs:domain rdf:resource="#sportleague"/>
			<rdfs:domain rdf:resource="#sportclub"/>
			<rdfs:domain rdf:resource="#sportteam"/>
			<rdfs:range  rdf:resource="xsd:int"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="locationcity">
			<rdfs:domain rdf:resource="#sportleague"/>
			<rdfs:domain rdf:resource="#sportclub"/>
			<rdfs:domain rdf:resource="#sportteam"/>
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
	
	<!-- definicion de propiedades estaticas para la clase person-->
	
		<rdf:Property rdf:ID="playerstatus">
			<rdfs:domain rdf:resource="#person"/>		
			<rdfs:range  rdf:resource="xsd:double"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="age">
			<rdfs:domain rdf:resource="#person"/>		
			<rdfs:range  rdf:resource="xsd:date"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="sex">
			<rdfs:domain rdf:resource="#person"/>		
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="id">
			<rdfs:domain rdf:resource="#person"/>		
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
	
	<!-- definicion de propiedades estaticas para la clase teammember-->
	
		<rdf:Property rdf:ID="number">
			<rdfs:domain rdf:resource="#teammember"/>		
			<rdfs:range  rdf:resource="xsd:int"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="mvp">
			<rdfs:domain rdf:resource="#teammember"/>		
			<rdfs:range  rdf:resource="xsd:string"/>
		</rdf:Property>
	
	<!-- definicion de propiedades estaticas para la clase teammember-->
	
		<rdf:Property rdf:ID="activeyear">
			<rdfs:domain rdf:resource="#sportmanager"/>		
			<rdfs:range  rdf:resource="xsd:date"/>
		</rdf:Property>
		
		<rdf:Property rdf:ID="birthdate">
			<rdfs:domain rdf:resource="#sportmanager"/>		
			<rdfs:range  rdf:resource="xsd:date"/>
		</rdf:Property>		
			
		<rdf:Property rdf:ID="chairperson">
				<rdfs:domain rdf:resource="#sportleague"/>
				<rdfs:domain rdf:resource="#sportclub"/>
				<rdfs:domain rdf:resource="#sportteam"/>
				<rdfs:range  rdf:resource="#person"/>
		</rdf:Property>
		
		
		

</rdf:RDF>

	
	
	
	
	
	
	
